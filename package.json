{
  "name": "antoinezanardi.fr",
  "version": "1.1.1",
  "description": "My personal portfolio built with Nuxt.js",
  "readme": "README.md",
  "type": "module",
  "keywords": [
    "nuxt",
    "portfolio",
    "antoine",
    "zanardi",
    "typescript"
  ],
  "homepage": "https://antoinezanardi.fr",
  "license": "MIT",
  "author": {
    "name": "Antoine Zanardi",
    "email": "antoine.zanardi@epitech.eu",
    "url": "https://antoinezanardi.fr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoinezanardi/antoinezanardi.fr.git"
  },
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "nuxt dev --port 4000",
    "typecheck": "nuxt typecheck",
    "build": "nuxi build",
    "lint": "npx eslint --config eslint.config.ts .",
    "lint:fix": "npx eslint --config eslint.config.ts --fix .",
    "lint:staged:fix": "lint-staged -c config/lint-staged/.lintstagedrc.eslint.json",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "test:unit": "cross-env NODE_ENV=test vitest --config config/vitest/vitest.global-unit-tests-config.ts",
    "test:unit:watch": "cross-env NODE_ENV=test vitest --watch --config config/vitest/vitest.global-unit-tests-config.ts",
    "test:unit:cov": "cross-env NODE_ENV=test rimraf tests/unit/coverage && vitest --coverage --config config/vitest/vitest.global-unit-tests-config.ts",
    "test:unit:staged": "lint-staged -c config/lint-staged/.lintstagedrc.unit-tests.json",
    "test:stryker": "cross-env NODE_ENV=test rimraf tests/stryker/coverage && stryker run config/stryker/stryker.conf.mjs",
    "test:stryker:force": "cross-env NODE_ENV=test rimraf tests/stryker/coverage tests/stryker/incremental/incremental.json && stryker run config/stryker/stryker.conf.mjs",
    "test:cucumber:prepare": "playwright install --with-deps chromium",
    "test:cucumber": "cross-env NODE_OPTIONS='--import tsx/esm' cucumber-js --config config/cucumber/cucumber.json; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:html-report": "tsx tests/acceptance/plugins/html-reporter/cucumber-html-reporter.ts",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "playwright:codegen": "npx playwright codegen http://localhost:4000",
    "script:create-branch": "scripts/create-git-branch.sh",
    "script:create-pull-request": "scripts/create-pull-request.sh",
    "script:create-release-changelog": "scripts/create-release-changelog.sh",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.7",
    "is-ci": "^4.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cucumber/cucumber": "^11.3.0",
    "@eslint/config-inspector": "^1.1.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/i18n": "^9.5.6",
    "@playwright/test": "^1.54.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stryker-mutator/api": "^9.0.1",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/typescript-checker": "^9.0.1",
    "@stryker-mutator/vitest-runner": "^9.0.1",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/aos": "^3.0.7",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^22.16.4",
    "@types/pngjs": "^6.0.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^7.0.3",
    "cucumber-html-reporter": "^7.2.0",
    "dev-stamp": "^1.2.0",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^10.3.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jest-extended": "^6.0.0",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "nuxt": "^3.17.7",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.54.1",
    "playwright-core": "^1.54.1",
    "pngjs": "^7.0.0",
    "radash": "^12.1.1",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "semantic-release": "24.2.7",
    "semantic-release-export-data": "^1.1.1",
    "tsx": "^4.20.3",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "validate-branch-name": "^1.3.2",
    "vite": "6.3.6",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "3.0.2"
  },
  "engines": {
    "node": ">=22.17.1"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "packageManager": "pnpm@9.15.9"
}