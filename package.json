{
  "name": "antoinezanardi.fr",
  "version": "1.0.0",
  "description": "My personal portfolio built with Nuxt.js",
  "readme": "README.md",
  "type": "module",
  "keywords": [
    "nuxt",
    "portfolio",
    "antoine",
    "zanardi",
    "typescript"
  ],
  "homepage": "https://antoinezanardi.fr",
  "license": "MIT",
  "author": {
    "name": "Antoine Zanardi",
    "email": "antoine.zanardi@epitech.eu",
    "url": "https://antoinezanardi.fr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoinezanardi/antoinezanardi.fr.git"
  },
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "nuxt dev --port 4000",
    "typecheck": "nuxt typecheck",
    "build": "nuxi typecheck && nuxi generate && cp .appengine/app.yaml .output/public",
    "lint": "npx eslint --config eslint.config.js .",
    "lint:fix": "npx eslint --config eslint.config.js --fix .",
    "lint:staged:fix": "lint-staged -c config/lint-staged/.lintstagedrc.eslint.json",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "script:create-branch": "scripts/create-git-branch.sh",
    "script:create-pull-request": "scripts/create-pull-request.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "is-ci": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.0",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/i18n": "^9.1.4",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@types/aos": "^3.0.4",
    "@types/bootstrap": "^5.2.6",
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "eslint": "^9.19.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nuxt": "^3.15.4",
    "rimraf": "^6.0.1",
    "sass": "^1.83.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^2.2.0"
  },
  "engines": {
    "node": ">=23.3.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "packageManager": "pnpm@9.14.2"
}